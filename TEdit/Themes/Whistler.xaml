                                </Trigger.EnterActions>
                                <Setter Property="Visibility" TargetName="IndeterminateRoot" Value="Visible"/>
                                <Setter Property="Visibility" TargetName="DeterminateRoot" Value="Collapsed"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

