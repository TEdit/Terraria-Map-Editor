                                            <GradientStop Color="#FFBDF4C0" Offset="0.198"/>
                                            <GradientStop Color="#FF9CEBAB" Offset="0.516"/>
                                            <GradientStop Color="#FF00C523" Offset="0.521"/>
                                            <GradientStop Color="#FF00D724" Offset="0.797"/>
                                            <GradientStop Color="#FF10DC2E" Offset="1"/>
                                        </LinearGradientBrush>
                                    </Rectangle.Fill>
                                </Rectangle>
