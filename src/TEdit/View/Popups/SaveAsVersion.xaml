<Window x:Class="TEdit.UI.Xaml.SaveAsVersionGUI"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TEdit.UI.Xaml"
        WindowStartupLocation="CenterOwner"
        mc:Ignorable="d"
        Background="{StaticResource ControlBackgroundBrush}" Foreground="{DynamicResource TextBrush}"
        Title="Select World Version" Height="450" Width="400">
    <Grid>
        <ScrollViewer Grid.ColumnSpan="1">
            <WrapPanel Name="ButtonPanel" HorizontalAlignment="Center" Margin="0,5,0,0" />
        </ScrollViewer>
    </Grid>
</Window>
